, value: \"novo\" },\n      ],\n    }\n  ];\n\n  const [selectedGroup, setselectedGroup] = useState(null);\n  const [selectedMulti, setselectedMulti] = useState(null);\n\n  function handleSelectGroup(selectedGroup) {\n    setselectedGroup(selectedGroup);\n  }\n\n  function handleMulti(selectedMulti) {\n    setselectedMulti(selectedMulti);\n  }\n\n  return (\n    <>\n      {/*//?=============================================================*/}\n      <Label htmlFor=\"billing-name\" md=\"3\" className=\"col-form-label\">\n        <CardTitle>Catálogo de endereços: </CardTitle>\n      </Label>\n      <Label htmlFor=\"billing-name\" md=\"6\" className=\"col-form-label\">\n        <Select\n          value={selectedGroup}\n          onChange={(s) => {\n            handleSelectGroup(s);\n          }}\n          options={optionGroup}\n          placeholder=\"Selecione o catálogo\"\n          classNamePrefix=\"select2-selection\"\n        />\n      </Label>\n\n      <div className=\"wizard clearfix\">\n        <div className=\"steps clearfix\">\n          <ul>\n            <NavItem>\n              <NavLink\n                className={classnames({\n                  active: activeTabProgress === 1,\n                })}\n                onClick={() => {\n                  toggleTabProgress(1);\n                }}\n              >\n                <span className=\"number\">01.</span> Contatos\n              </NavLink>\n            </NavItem>\n            <NavItem>\n              <NavLink\n                className={classnames({\n                  active: activeTabProgress === 2,\n                })}\n                onClick={() => {\n                  toggleTabProgress(2);\n                }}\n              >\n                <span className=\"number\">02.</span>\n                <span>Adicionar</span>\n              </NavLink>\n            </NavItem>\n          </ul>\n          {/*//*================================================*/}\n          <div id=\"bar\" className=\"mt-4\">\n            <Progress color=\"success\" striped animated value={progressValue} />\n            <div className=\"progress-bar bg-success progress-bar-striped progress-bar-animated\" />\n          </div>\n          <TabContent\n            activeTab={activeTabProgress}\n            className=\"twitter-bs-wizard-tab-content\"\n          >\n            <TabPane tabId={1}>\n              <TabelaContatos />\n            </TabPane>\n\n            <TabPane tabId={2}>\n              <div>\n                <Form className=\"mt-4\">\n                  <FormGroup className=\"mb-4\" row>\n                    <Label\n                      htmlFor=\"billing-phone\"\n                      md=\"2\"\n                      className=\"col-form-label\"\n                    >\n                      E-mail:\n                    </Label>\n                    <Col md={10}>\n                      <input\n                        type=\"text\"\n                        className=\"form-control\"\n                        id=\"billing-phone\"\n                        placeholder=\"E-mail\"\n                      />\n                    </Col>\n                  </FormGroup>\n                  <FormGroup className=\"mb-4\" row>\n                    <Label\n                      htmlFor=\"billing-phone\"\n                      md=\"2\"\n                      className=\"col-form-label\"\n                    >\n                      Telefone:\n                    </Label>\n                    <Col md={10}>\n                      <input\n                        type=\"text\"\n                        className=\"form-control\"\n                        id=\"billing-phone\"\n                        placeholder=\"Teleone\"\n                      />\n                    </Col>\n                  </FormGroup>\n                  <FormGroup className=\"mb-4\" row>\n                    <Label\n                      htmlFor=\"billing-phone\"\n                      md=\"2\"\n                      classNam